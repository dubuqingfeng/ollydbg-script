Getpacktype: //添加获取壳类型
inc rreflag
bphwcall

mov tmp,[esp+8]
add tmp,40
push eax
findtypeloop:
mov al,[tmp]
cmp al,20
jne findpacktype
inc tmp
jmp findtypeloop

findpacktype:
cmp al,57
je findtypewl
cmp al,54
je findtypetmd
pop eax
jmp valnext

findtypewl:
pop eax
mov [tmp+17],0
eval "{[tmp]}"
mov packtype,$RESULT
jmp valnext

findtypetmd:
