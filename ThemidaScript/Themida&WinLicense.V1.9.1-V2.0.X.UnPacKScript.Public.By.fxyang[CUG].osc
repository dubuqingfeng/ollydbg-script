Getpackver: //添加壳版本获得,by qql504 感谢!
mov  temp, [esp+8]
mov  tmp, [esp+8]
GMEMI temp, MEMORYBASE
mov   temp, $RESULT
find  temp, #000004000000322E#          //特征码
cmp  $RESULT, 0
jz   next
jmp  ver
next:
find temp, #000004000000312E#          //特征码
cmp $RESULT, 0
jz  stop
ver:
mov temp, $RESULT
add temp, 6
eval "{[temp]}"
mov packver,$RESULT
ret

GetImageType: //获得镜像类型,这个代码抄别人的。感谢!
var characteristics
mov tmp,modulebase
add tmp, 3C
mov tmp, [tmp]
add tmp, modulebase
add tmp,16
mov characteristics,[tmp]
and characteristics,0FFFF
and characteristics,2000
cmp characteristics,2000
je  ImageType_DLL
mov Imgtype,"exe"
ret
